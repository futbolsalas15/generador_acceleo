[comment encoding = UTF-8 /]
[module generador('http://www.eclipse.org/uml2/5.0.0/UML')]

[template public gen(p : Package)]
[comment @main/]
[file ('/documento.html', false, 'UTF-8')]
	<html>
	<body>
		<ul>
		[p.packagedElement->filter(Class).generarNombre()/] 
		</ul>
	</body>
	</html>
[/template]

[template public generarNombre(c: Class)]
	<li>[c.name/]</li>
[/template]
